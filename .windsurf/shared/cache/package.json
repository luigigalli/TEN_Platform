{
  "name": "rest-express",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "predev": "tsx scripts/refresh-context.ts",
    "dev": "concurrently \"tsx server/index.ts\" \"vite\"",
    "dev:server": "tsx server/index.ts",
    "dev:client": "vite",
    "prebuild": "tsx scripts/refresh-context.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "premigrate": "tsx scripts/refresh-context.ts",
    "migrate": "tsx db/migrate.ts",
    "prestart": "tsx scripts/refresh-context.ts",
    "start": "NODE_ENV=production node dist/index.js",
    "start:sandbox": "NODE_ENV=sandbox tsx app.ts",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "verify-env": "tsx scripts/verify-env",
    "db:pull": "tsx scripts/db-sync.ts pull",
    "db:push-replit": "tsx scripts/db-sync.ts push",
    "sync:windsurf": "bash scripts/sync-windsurf.sh",
    "sync:replit": "bash scripts/sync-replit.sh",
    "check-users": "tsx scripts/check-users.ts",
    "pretest": "tsx scripts/refresh-context.ts",
    "test": "NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json mocha -r ts-node/register 'tests/**/*.test.ts'",
    "test:unit": "NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json mocha -r ts-node/register 'tests/**/*.test.ts' --grep 'unit'",
    "test:integration": "NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json mocha -r ts-node/register 'tests/**/*.test.ts' --grep 'integration'",
    "test:sandbox": "NODE_ENV=sandbox TS_NODE_PROJECT=tsconfig.test.json mocha -r ts-node/register 'tests/**/*.test.ts' --grep 'sandbox'",
    "test:watch": "NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json mocha -r ts-node/register 'tests/**/*.test.ts' --watch",
    "test:coverage": "NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json c8 mocha -r ts-node/register 'tests/**/*.test.ts'",
    "openapi": "openapi-generator-cli generate -i server/openapi/spec.yaml -g typescript-node -o generated/api"
  },
